{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "allowJs": true,
    "sourceMap": false,
    "resolveJsonModule": true,
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "noEmit": false,
    "baseUrl": ".",
    "outDir": "tsc-compiled",
    "paths": {
      "@/*": [
        "../src/*"
      ]
    },
    "types": [
      "@types/node"
    ]
  }
}
