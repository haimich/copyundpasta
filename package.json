    "@nuxtjs/axios": "^5.5.4",
    "@nuxtjs/google-analytics": "^2.2.0",
    "@nuxtjs/sitemap": "^1.2.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.0.0",
    "element-ui": "^2.11.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.0.0",
    "feed": "^3.0.0",
    "helmet": "^3.20.0",
    "knex": "^0.17.6",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.0",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.8.1",
    "uniqid": "^5.0.3",
    "vue-property-decorator": "^8.2.1",
