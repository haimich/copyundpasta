{
  "name": "copyundpasta",
  "version": "1.0.0",
  "description": "My little foodblog",
  "author": "Michael MÃ¼ller",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "debug": "nodemon --inspect node_modules/.bin/nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "deploy": "node scripts/deploy.js",
    "migrate": "npm run db:migrate && npm run elastic:rebuild",
    "db:rebuild": "knex migrate:rollback --all && npm run db:migrate && npm run db:seed",
    "db:migrate": "knex migrate:latest",
    "db:seed": "npm run clean:seeds && cd knex_seeds && tsc && knex seed:run",
    "elastic:rebuild": "node scripts/rebuildElasticMapping.js",
    "clean": "npm run clean:build && npm run clean:seeds",
    "clean:build": "rm -rf .nuxt",
    "clean:seeds": "rm -rf knex_seeds/compiled",
    "checkBrokenLinks": "blc https://www.copyundpasta.de -ro --exclude sitemap --exclude rss.xml --exclude atom.xml",
    "generate": "nuxt-ts generate",
    "test": "jest",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.4.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@nuxt/typescript-runtime": "^0.2.1",
    "@nuxtjs/axios": "^5.6.0",
    "@nuxtjs/google-analytics": "^2.2.1",
    "@nuxtjs/sitemap": "^2.0.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.1.0",
    "element-ui": "^2.11.1",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^5.0.0",
    "feed": "^4.0.0",
    "helmet": "^3.21.1",
    "knex": "^0.19.5",
    "lodash": "^4.17.19",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "mysql": "^2.17.1",
    "nodemailer": "^6.3.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^2.0.0",
    "uniqid": "^5.0.3",
    "vue-property-decorator": "^8.2.2",
    "vue-recaptcha": "^1.2.0",
    "vue-timeago": "^5.1.2",
    "vueperslides": "^1.20.3"
  },
  "devDependencies": {
    "@nuxt/typescript-build": "^0.3.1",
    "@nuxtjs/eslint-config-typescript": "^0.1.3",
    "@types/jest": "^24.0.18",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.3",
    "broken-link-checker": "^0.7.8",
    "element-theme-chalk": "^2.12.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.2.3",
    "jest": "^24.9.0",
    "node-sass": "^4.12.0",
    "node-ssh": "^6.0.0",
    "nodemon": "^1.19.3",
    "ora": "^4.0.2",
    "prettier": "^1.18.2",
    "sass-loader": "^8.0.0",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.1.0",
